    <link rel="stylesheet" href="/static/contact.css">
    <%- include('partials/head') %>
    <%- include('partials/navbar') %>
</head>
<body style="margin-top: 60px;">


  <script src="	https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>

    <section class="contact">
     <div class="content">
        <h2>Contact Me!</h2>
        <p>I'm here to help and answer any questions you might have.  <br>
        I look forward to hearing from you!</p>
       
     </div>
     <div class="container">
        <div class="contactInfo">
            <div class="box">
                <div class="icon"><i class="fa fa-phone" aria-hidden="true"></i></div>
                    <div class="text">
                        <h3>Phone</h3>
                      
                        <p>+923163269520</p>
                    </div>
                </div>
                <div class="box">
                    <div class="icon"> <i class="fa fa-envelope-o" aria-hidden="true"></i> </div>
                        <div class="text">
                            <h3>Email</h3>
                            <p>asmanzoor@outlook.com</p>
                        </div>
                    </div>
                    <div class="box">
                        <div class="icon"><i class="fa fa-linkedin" aria-hidden="true"></i> </div>
                            <div class="text">
                                <h3>Linkedin</h3>
                                <p>linkedin.com/in/aiman-shaikh</p>
                            </div>
                       
            </div>
        </div>
     <div class="contactForm">
        <form action="https://api.web3forms.com/submit" method="POST">
            <h2>Send Message!</h2>
            <input type="hidden" name="access_key" value="07957487-ab97-4b20-8e0e-82eff1b7a41c">
            <div class="inputBox">
                <input type="text" name="fullName" required="required" >
                <span>Full Name</span>
            </div>
             <div class="inputBox">
                <input type="text" name="email" required="required">
                <span>Email</span>
            </div>
             <div class="inputBox">
               <textarea required="required" name="text" id="" ></textarea>
                <span>Type your message...</span>
            </div>
             <div class="inputBox">
                <!-- <input type="submit" value="send" name="" required="required"> -->
                <a href="/thanks">   <button type="submit" required="required">Send</button></a>
            </div>
        </form>
     </div>
    </section><footer>
        <p>&copy; 2023 Aiman Shaikh</p>
      </footer>
</body>

</html>


